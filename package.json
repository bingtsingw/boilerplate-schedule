{
  "name": "schedule",
  "scripts": {
    "build": "malagu build",
    "clean": "rimraf lib dist .malagu",
    "deploy:development": "malagu deploy -m fc -m development",
    "deploy:production": "malagu deploy -m fc -m production",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "prepare": "if [ -d './node_modules/husky' ]; then husky install; fi",
    "start": "malagu serve"
  },
  "dependencies": {
    "@malagu/core": "^2.20.5",
    "@malagu/schedule": "^2.20.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@malagu/cli": "^2.20.5",
    "commitizen": "^4.2.4",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.2",
    "prettier-plugin-organize-imports": "^2.3.4",
    "prettier-plugin-packagejson": "^2.2.17"
  }
}
